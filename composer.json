{
    "name": "ericvvc9/voyager-bread-generator",
    "description": "Create BREAD from the console using artisan",
    "type": "library",
    "require": {
        "ericvvc9/voyager": "^1.4"
    },
    "require-dev": {
        "laravel/laravel": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Ochoa",
            "email": "jeffer.8a@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "VoyagerBread\\": "src/"
        }
      },
      "extra": {
        "hook": {
          "providers": [
            "VoyagerBread\\BreadGeneratorServiceProvider"
          ]
        },
        "laravel": {
            "providers": [
                "VoyagerBread\\BreadGeneratorServiceProvider"
            ]
        }
      }
}
